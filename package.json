{
  "name": "react-sadness",
  "version": "1.0.0-rc.0",
  "description": "useRequest hook & set of components for requesting API data within React applications",
  "scripts": {
    "build-storybook": "build-storybook -c .storybook/ -o build/",
    "now": "now",
    "postpublish": "rm -rf lib/",
    "prepublishOnly": "babel src/ -d lib/ --compact true --delete-dir-on-start --ignore \"src/**/*.test.js\" --minified --no-comments",
    "reinstall": "rm -rf node_modules/ package-lock.json && npm i",
    "storybook": "start-storybook -p 6006"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "main": "lib/index.js",
  "source": "src/index.js",
  "dependencies": {
    "axios": "^0.21.0",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.0",
    "qs": "^6.9.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^6.0.19",
    "@storybook/addon-links": "^6.0.19",
    "@storybook/addon-storysource": "^6.0.19",
    "@storybook/addons": "^6.0.19",
    "@storybook/react": "^6.0.19",
    "babel-loader": "^8.1.0"
  },
  "author": "Igor Davydenko <iam@igordavydenko.com> (http://igordavydenko.com/)",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/playpauseandstop/react-sadness#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playpauseandstop/react-sadness.git"
  },
  "bugs": {
    "url": "https://github.com/playpauseandstop/react-sadness/issues"
  },
  "keywords": [
    "react",
    "api",
    "axios"
  ],
  "alias": {
    "react-sadness": "./src/"
  }
}
